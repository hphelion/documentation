<task-wr ditaarch:DITAArchVersion="1.2" domains="(topic task task-wr)                            (topic hi-d)                            (topic pr-d)                            (topic sw-d)                            (topic ui-d)                            (topic wr-sw-d)                            (topic xml-d)   " id="jow1404333628570" xml:lang="en-us" xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">
