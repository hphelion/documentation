                    <colspec colname="col1" colwidth="14*"/>
                    <colspec colname="col2" colwidth="60*"/>
                    <colspec colname="col3" colwidth="26*"/>
                    <thead>
                        <row>
                            <entry colname="col1" ixia_locid="83">
                                <p ixia_locid="84">Menu item</p>
                            </entry>
                            <entry colname="col2" ixia_locid="85">
                                <p ixia_locid="86">Description</p>
                            </entry>
                            <entry colname="col3" ixia_locid="87">
                                <p ixia_locid="88">Reference</p>
                            </entry>
                        </row>
                    </thead>
                    <tbody>
                        <row>
                            <entry ixia_locid="89">
                                <p ixia_locid="90">Stacks</p>
                            </entry>
                            <entry ixia_locid="91">
                                <p ixia_locid="92">Details for existing services (stacks), and controls for creating new services
                  (launching stacks). Service details include topologies, resource allocations, and
                  event histories.</p>
                            </entry>
                            <entry ixia_locid="93">
                                <p ixia_locid="94"><xref href="jow1404333804266.xml" ixia_locid="103"/></p>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </section>
